{
  "version": 1,
  "ds": {
    "refresh_delay_ms": 30000,
    "cluster": {
      "name": "ds",
      "type": "strict_dns",
      "tls": false,
      "connect_timeout_ms": 1000,
      "lb_type": "round_robin",
      "hosts": [
        {
          "url": "tcp://nginx:80"
        }
      ]
    }
  },
  "statsd": {
    "name": "statsd",
    "connect_timeout_ms": 1000,
    "type": "strict_dns",
    "tls": false,
    "lb_type": "round_robin",
    "hosts": [
      {
        "url": "tcp://relay:2000"
      }
    ]
  },
  "listener": {
    "address": "tcp://0.0.0.0:9211",
    "access_log_path": "/dev/stdout"
  },
  "admin": {
    "address": "tcp://0.0.0.0:9901",
    "access_log_path": "/dev/stdout"
  }
}
